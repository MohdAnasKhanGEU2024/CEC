#include<iostream>
using namespace std;

void InsertionSort(int arr[], int n)
{
    for(int i = 1; i < n; i++)
    {
        int m = arr[i];
        int j = i-1;
        while(j>=0 && arr[j]>m)
        {
            arr[j+1] = arr[j];
            j--;
        }
        arr[j+1] = m;
    }
}

int main()
{
    int n = 5;
    int arr[n] = {5,4,9,7,2};
    for(int i = 0; i < n; i++)
    {
        cout<<arr[i]<<" ";
    }
    cout<<endl;
    InsertionSort(arr,n);
    for(int i = 0; i < n; i++)
    {
        cout<<arr[i]<<" ";
    }
    cout<<endl;
}
